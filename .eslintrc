{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "root": true,
  "env": {
    "node": true,
    "browser": true
  },
  "globals": {},
  "rules": {
    "semi": [2, "never"],
    "max-len": [2, 120, 2, {
      "ignoreUrls": true,
      "ignoreComments": false
    }],
    "jsx-quotes": [2, "prefer-single"],
    "react/jsx-filename-extension": 0
  }
}
